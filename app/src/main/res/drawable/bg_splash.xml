<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:type="sweep"
                android:centerX="0.5"
                android:centerY="0.5"
                android:startColor="#FF4B6C"
                android:centerColor="#FF8959"
                android:endColor="#FFB344"
                android:gradientRadius="100%p"/>
        </shape>
    </item>
    <item android:gravity="center">
        <shape android:shape="oval">
            <gradient
                android:type="radial"
                android:centerX="0.5"
                android:centerY="0.5"
                android:startColor="#26FFFFFF"
                android:endColor="#00FFFFFF"
                android:gradientRadius="100%p"/>
        </shape>
    </item>
</layer-list>